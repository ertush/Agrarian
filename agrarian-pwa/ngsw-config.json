{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/assets/**",
        "/*.bundle.css",
        "/*.bundle.js",
        "/*.chunk.js",
          "/manifest.webmanifest"
      ]
    }
  }],
  "dataGroups": [{
      "name": "api-freshness",
      "urls": [
        "https://api.github.com/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "3d",
        "timeout": "30s"
      }
    }]
}
